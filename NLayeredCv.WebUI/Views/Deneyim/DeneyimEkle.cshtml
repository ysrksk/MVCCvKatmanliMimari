@using NLayeredCv.Entities.Concrete
@model TblDeneyimlerim
@{
    /**/

    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Deneyim Formu</h2>
<br />
@using (Html.BeginForm("DeneyimEkle", "Deneyim", FormMethod.Post))
{
    <div class="form-group">
        @Html.Label("Başlık")
        @Html.TextBoxFor(m => m.Baslik, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.Label("Alt Başlık")
        @Html.TextBoxFor(m => m.AltBaslik, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.Label("Tarih")
        @Html.TextBoxFor(m => m.Tarih, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.Label("Açıklama")
        @Html.TextAreaFor(m => m.Aciklama, new { @class = "form-control" })
    </div>
    <br />
    <button type="submit" class="btn btn-info">Kaydet</button>

}

